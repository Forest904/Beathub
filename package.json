{
  "name": "cd-collector",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "pnpm": {
    "overrides": {
      "@babel/core": "7.24.7"
    }
  },
  "scripts": {
    "postinstall": "pnpm --filter @cd-collector/shared build",
    "shared:build": "pnpm --filter @cd-collector/shared build",
    "shared:watch": "pnpm --filter @cd-collector/shared exec tsc --project tsconfig.json --watch",
    "dev:web": "pnpm --filter web start",
    "build:web": "pnpm --filter web build",
    "dev:mobile": "pnpm --filter @cd-collector/mobile start",
    "mobile:start": "pnpm --filter @cd-collector/mobile start",
    "mobile:android": "pnpm --filter @cd-collector/mobile android",
    "mobile:apk:dev": "pnpm --filter @cd-collector/mobile exec eas build --platform android --local --profile dev-apk",
    "mobile:apk:release": "pnpm --filter @cd-collector/mobile exec eas build --platform android --local --profile release-apk",
    "lint": "eslint \"{web,apps/mobile,packages}/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --check \"{web,apps/mobile,packages}/**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:write": "prettier --write \"{web,apps/mobile,packages}/**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "devDependencies": {
    "@eslint/js": "8.57.0",
    "@typescript-eslint/eslint-plugin": "8.14.0",
    "@typescript-eslint/parser": "8.14.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "prettier": "3.4.1",
    "typescript": "5.6.3"
  }
}
