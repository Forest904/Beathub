{
  "id": null,
  "title": "CD-Collector SaaS Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "stat",
      "title": "Download Attempts (5m)",
      "id": 1,
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "sum(increase(cdcollector_download_attempts_total[5m]))",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 }
    },
    {
      "type": "stat",
      "title": "Queue Depth",
      "id": 2,
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "cdcollector_job_queue_depth",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 }
    },
    {
      "type": "timeseries",
      "title": "Job Execution Duration (p95)",
      "id": 3,
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(cdcollector_download_job_execution_seconds_bucket[5m])) by (le))",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 6 }
    },
    {
      "type": "timeseries",
      "title": "Queue Wait Time (p95)",
      "id": 4,
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(cdcollector_job_queue_wait_seconds_bucket[5m])) by (le))",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 10, "w": 12, "h": 6 }
    },
    {
      "type": "timeseries",
      "title": "Download Success / Failure",
      "id": 5,
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "rate(cdcollector_download_success_total[5m])",
          "refId": "A",
          "legendFormat": "success"
        },
        {
          "expr": "rate(cdcollector_download_failure_total[5m])",
          "refId": "B",
          "legendFormat": "failure"
        }
      ],
      "gridPos": { "x": 0, "y": 16, "w": 12, "h": 6 }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "label": "Prometheus",
        "refresh": 1
      }
    ]
  }
}
